{
  "name": "@0x33.io/wasm-sdk",
  "version": "0.0.11",
  "author": "Ivan Prodanov",
  "description": "Main WASM package.",
  "main": "dist/wasm-sdk.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "preclean": "yarn install --mutex network",
    "clean": "node node_modules/rimraf/bin.js ./dist",
    "prebuild": "yarn run clean && yarn install --mutex network",
    "build": "yarn run build:production && yarn run build:sdk",
    "build:debug": "node node_modules/webpack-cli/bin/cli.js --mode development --config ./webpack.config.js",
    "build:production": "node node_modules/webpack-cli/bin/cli.js --mode production --config ./webpack.config.js",
    "build:sdk": "build.bat",
    "test": "jest",
    "packme": "yarn build && npm pack",
    "packme:dry": "yarn build && npm pack --dry-run",
    "publishme": "yarn build && npm publish"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.4",
    "rimraf": "^2.7.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {},
  "wasm": true
}
