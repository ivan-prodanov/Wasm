{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "target": "ES2016",
    "lib": ["es2017", "dom", "WebWorker.ImportScripts"],
    "strict": true,
    "outDir": "dist",
    "baseUrl": "src"
  },
  "include": ["src"],
}